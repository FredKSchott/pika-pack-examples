{
  "name": "simple-cli",
  "description": "A simple example Pika CLI package",
  "version": "1.0.0",
  "scripts": {},
  "license": "MIT",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "my-example-cli"
        }
      ]
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@pika/plugin-standard-pkg": "^0.3.0",
    "@pika/plugin-build-node": "^0.3.0",
    "@pika/plugin-simple-bin": "^0.3.0"
  }
}
